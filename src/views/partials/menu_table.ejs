<table id="menu-table" class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Start Date</th>
      <th scope="col">End Date</th>
      <th scope="col">Created Date</th>
      <th scope="col">Number Of Days</th>
      <th scope="col">Print</th>
      <th scope="col">Delete</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i = 0; i < menus.length; i++) { %>
      <tr class="menu-row" data-menu-id="<%= menus[i]._id %>">
        <td><%= dateformat(menus[i].start_date, 'mm-dd-yyyy') %></td>
        <td><%= dateformat(menus[i].end_date, 'mm-dd-yyyy') %></td>
        <td><%= dateformat(menus[i].date_created, 'mm-dd-yyyy') %></td>
        <td><%= menus[i].days_covered %></td>
        <td>
          <a class="btn btn-info printMenuButton" href="/menu/print/<%= menus[i]._id %>">
            <i class="fas fa-print"></i> print
          </a>
        </td>
        <td>
          <button class="btn btn-danger deleteMealButton" >
            <i class="far fa-trash-alt"></i> delete
          </button>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>
